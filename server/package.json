{
  "name": "mna-doctrina-server",
  "version": "0.0.1",
  "description": "[MNA] App Template",
  "repository": "https://github.com/mission-apprentissage/doctrina.git",
  "private": false,
  "author": "MNA",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "nodemon --ext 'ts,js,cjs,json' --ignore 'tests/' --exec 'ts-node --esm src/index.ts'",
    "build": "yarn clean && tsc -p . && yarn copy-assets",
    "start": "node build/index.js",
    "copy-assets": "copyfiles -u 2 src/assets/api-docs/swagger.json src/assets/templates/*.ejs build/assets",
    "clean": "rimraf build/",
    "test": "mocha --recursive --exit --timeout 5000 tests/",
    "cli": "DOCTRINA_LOG_DESTINATIONS=stderr node src/cli.js",
    "coverage": "nyc --temp-dir .coverage/.nyc_output --report-dir .coverage --reporter=lcov --reporter=html yarn test",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.9.1",
    "@sentry/node": "^5.22.3",
