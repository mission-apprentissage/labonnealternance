{
  "title": "go-to-faq",
  "steps": [
    {
      "type": "setViewport",
      "width": 1510,
      "height": 1304,
      "deviceScaleFactor": 1,
      "isMobile": false,
      "hasTouch": false,
      "isLandscape": false
    },
    {
      "type": "navigate",
      "url": "https://labonnealternance.apprentissage.beta.gouv.fr/",
      "assertedEvents": [
        {
          "type": "navigation",
          "url": "https://labonnealternance.apprentissage.beta.gouv.fr/",
          "title": "La bonne alternance | Trouvez votre alternance"
        }
      ]
    },
    {
      "type": "click",
      "target": "main",
      "selectors": [
        ["aria/Accès à la foire aux questions"],
        ["li:nth-of-type(5) > a"],
        ["xpath///*[@id=\"__next\"]/main/div/footer/div[2]/nav/div/div[1]/ul/li[5]/a"],
        ["text/FAQ"]
      ],
      "offsetY": 12.8125,
      "offsetX": 16.796875
    },
    {
      "type": "waitForElement",
      "target": "main",
      "selectors": [["aria/Questions fréquemment posées"], ["h1"], ["xpath///*[@id=\"__next\"]/main/div/div[3]/div/div[1]/h1"], ["pierce/h1"], ["text/Questionsfréquemmentposées"]]
    }
  ]
}
