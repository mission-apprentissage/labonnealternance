{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docker-dev": "yarn install && yarn dev",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.js",
    "start": "next start",
    "lint": "next lint",
    "test": "nyc --silent jest --env=jest-environment-jsdom-sixteen",
    "testlocal": "nyc --silent jest --env=jest-environment-jsdom-sixteen --watch",
    "test:ci": "jest --ci",
    "coverage": "jest --ci --coverage",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "echo '{ \"presets\": [ \"next/babel\" ], \"plugins\": [ \"istanbul\" ] }' > .babelrc && start-server-and-test dev http://localhost:3000 cypress ; rm .babelrc",
    "e2e:headless": "echo '{ \"presets\": [ \"next/babel\" ], \"plugins\": [ \"istanbul\" ] }' > .babelrc && nyc --silent --no-clean  start-server-and-test dev http://localhost:3000 cypress:headless ; rm .babelrc",
    "mergecov": "mkdir -p ./coverage/both && cp ./coverage/e2e/coverage-final.json ./coverage/both/coverage-final-e2e.json && cp ./coverage/unit/coverage-final.json ./coverage/both/coverage-final-unit.json && mkdir -p ./coverage/final && npx nyc merge ./coverage/both ./coverage/final/merged-coverage.json && npx nyc report -t ./coverage/final/ --report-dir ./coverage/final/ --reporter=html --reporter=text-summary"
  },
  "nyc": {
    "report-dir": "coverage/e2e",
    "exclude": [
      "pages",
      "pages/**"
    ],
    "excludeAfterRemap": true
  },
  "dependencies": {
    "@chakra-ui/icons": "2.0.11",
    "@chakra-ui/react": "2.3.6",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@turf/distance": "^6.0.1",
    "axios": "1.1.2",
    "bootstrap": "^4.4.1",
    "config": "3.3.3",
    "downshift": "5.2.0",
    "email-misspelled": "3.3.9",
    "formik": "^2.1.4",
    "framer-motion": "^6",
    "lodash": "4.17.20",
    "mapbox-gl": "2.10.0",
    "markdown-it": "12.0.4",
    "next": "12.3.1",
    "next-images": "1.8.4",
    "next-plausible": "3.6.3",
    "next-seo": "4.26.0",
    "next-sitemap": "1.6.133",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "11.4.2",
    "react-html-parser": "2.0.2",
    "react-notion-x": "4.12.0",
    "react-swipeable": "6.1.0",
    "reactstrap": "^8.4.1",
    "sass": "1.55.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.1.1",
    "@types/node": "18.8.2",
    "@types/react": "18.0.21",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "jest": "28.0.3",
    "jest-environment-jsdom": "28.0.2",
    "typescript": "4.8.4"
  }
}
