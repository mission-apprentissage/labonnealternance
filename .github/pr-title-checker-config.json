{
  "LABEL": {
    "name": ":warning: Pull request title needs formatting",
    "color": "D93F0B"
  },
  "CHECKS": {
    "prefixes": ["fix: ", "feat: ", "refactor: ", "chore: ", "docs: ", "build: ", "ci: ", "perf: ", "style: ", "test: ", "sync: "],
    "regexp": "^(fix|feat|refactor|chore|docs|build|ci|perf|style|test|sync)(\\([a-zA-Z0-9_-]+\\))?: .+"
  },
  "MESSAGES": {
    "success": "PR title OK",
    "failure": "Failing CI test",
    "notice": ""
  }
}
