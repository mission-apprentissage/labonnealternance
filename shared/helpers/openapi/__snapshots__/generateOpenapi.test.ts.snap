  "paths": {
    "/appointment-request/context/create": {
      "post": {
        "description": "Appointment request",
        "operationId": "appointmentCreateContext",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "anyOf": [
                  {
                    "additionalProperties": false,
                    "properties": {
                      "idParcoursup": {
                        "type": "string",
                      },
                      "referrer": {
                        "enum": [
                          "parcoursup",
                        ],
                        "type": "string",
                      },
                      "trainingHasJob": {
                        "type": "boolean",
                      },
                    },
                    "required": [
                      "idParcoursup",
                      "referrer",
                    ],
                    "type": "object",
                  },
                  {
                    "additionalProperties": false,
                    "properties": {
                      "idRcoFormation": {
                        "minLength": 1,
                        "type": "string",
                      },
                      "referrer": {
                        "enum": [
                          "pfr_pays_de_la_loire",
                        ],
                        "type": "string",
                      },
                      "trainingHasJob": {
                        "type": "boolean",
                      },
                    },
                    "required": [
                      "idRcoFormation",
                      "referrer",
                    ],
                    "type": "object",
                  },
                  {
                    "additionalProperties": false,
                    "properties": {
                      "idActionFormation": {
                        "minLength": 1,
                        "type": "string",
                      },
                      "referrer": {
                        "enum": [
                          "onisep",
                        ],
                        "type": "string",
                      },
                      "trainingHasJob": {
                        "type": "boolean",
                      },
                    },
                    "required": [
                      "idActionFormation",
                      "referrer",
                    ],
                    "type": "object",
                  },
                  {
                    "additionalProperties": false,
                    "properties": {
                      "idCleMinistereEducatif": {
                        "minLength": 1,
                        "type": "string",
                      },
                      "referrer": {
                        "enum": [
                          "parcoursup",
                          "lba",
                          "pfr_pays_de_la_loire",
                          "onisep",
                          "jeune_1_solution",
                          "affelnet",
                        ],
                        "type": "string",
                      },
                      "trainingHasJob": {
                        "type": "boolean",
                      },
                    },
                    "required": [
                      "idCleMinistereEducatif",
                      "referrer",
                    ],
                    "type": "object",
                  },
                ],
              },
            },
          },
          "required": true,
        },
        "responses": {
          "2xx": {
            "content": {
              "application/json": {
                "schema": {
                  "anyOf": [
                    {
                      "additionalProperties": false,
                      "properties": {
                        "cfd": {
                          "example": "24113401",
                          "type": "string",
                        },
                        "cle_ministere_educatif": {
                          "example": "101249P01313538697790003635386977900036-93006#L01",
                          "type": "string",
                        },
                        "code_postal": {
                          "example": "93170",
                          "type": "string",
